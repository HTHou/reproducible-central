groupId=io.takari
artifactId=io.takari.incrementalbuild
display=${groupId}:${artifactId}
version=1.0.2

gitRepo=https://github.com/takari/${artifactId}.git
gitTag=${artifactId}-${version}

tool=mvn
jdk=11
newline=lf
umask=022
timezone="Etc/GMT-1"

command="mvn clean package -DskipTests -Dmaven.javadoc.skip -Dgpg.skip -Dbuildinfo.detect.skip=false -Dbuildinfo.skipModules='*/*-its'"
buildinfo=target/${artifactId}-${version}.buildinfo

diffoscope=${artifactId}-${version}.diffoscope
issue=
